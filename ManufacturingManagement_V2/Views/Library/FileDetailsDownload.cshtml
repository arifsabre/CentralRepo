@model IEnumerable<ManufacturingManagement_V2.Models.LibFileUploadMDI>



<table id="jdtabledesktop" class="display nowrap" style="width:100%">
    <thead>
        <tr style="text-align:left">
            <th class="col-md-2" style="text-align:left">
                <label>Id</label>
                @*@Html.DisplayNameFor(model => model.RecordId)*@
            </th>
            <th class="col-md-2" style="text-align:left">
                <label>Category</label>
                @* @Html.DisplayNameFor(model => model.LibCategory)*@
            </th>
            <th class="col-md-2" style="text-align:left">
                <label>SubCategory</label>
                @*@Html.DisplayNameFor(model => model.LibSubCategory)*@
            </th>
            <th class="col-md-2" style="text-align:left">
                <label>Description</label>
                @*   @Html.DisplayNameFor(model => model.Description)*@
            </th>

            <th class="col-md-2" style="text-align:left">
                <label>Uploaded By</label>
                @*   @Html.DisplayNameFor(model => model.username)*@
            </th>

            <th class="col-md-2" style="text-align:left">
                <label>FileName</label>
                @* @Html.DisplayNameFor(model => model.FileName)*@
            </th>
            @*<th class="col-md-2" style="text-align:left">
                    <label>Download</label>
                     @Html.DisplayNameFor(model => model.FileName)
                </th>*@
            <th class="col-md-2" style="text-align:left">
                <label>Download</label>
                @* @Html.DisplayNameFor(model => model.FileName)*@
            </th>
            @*<th class="col-md-2" style="text-align:left">
                    @Html.DisplayNameFor(model => model.FileContent)
                </th>*@
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr style="text-align:left">
                <td style="text-align:left">
                    @Html.DisplayFor(modelItem => item.RecordId)
                </td>
                <td style="text-align:left">
                    @Html.DisplayFor(modelItem => item.LibCategory)
                </td>
                <td style="text-align:left">
                    @Html.DisplayFor(modelItem => item.LibSubCategory)
                </td>

                <td style="text-align:left">
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td style="text-align:left">
                    @Html.DisplayFor(modelItem => item.username)
                </td>
                <td style="text-align:left">
                    @Html.DisplayFor(modelItem => item.FileName)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.FileContent)
                    </td>*@
                <td style="text-align:left">
                        @Html.ActionLink("Download", "DownLoadFile", new { id = item.Id })
                    </td>
                @*<td style="text-align:left">
                    @Html.ActionLink("View", "GetFile", new { id = item.Id })
                </td>*@

            </tr>
        }
    </tbody>
</table>


