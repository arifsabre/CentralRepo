        function GetDetails(id) {
            $.ajax({
                url: "/LibraryMasterUpload/Edit_LibraryMasterFileRecord/" + id,
                typr: "GET",
                contentType: "application/json;charset=UTF-8",
                dataType: "json",
                success: function (response) {
                    $('#RecordId').val(response.RecordId);
                   // $('#ShortName').val(response.ShortName);
                   // $('#DepCode').val(response.DepCode);
                    $('#DoccumentName').val(response.DoccumentName);
                    $('#Location').val(response.Location);
                    //$('#OwnerName').val(response.OwnerName);
                   // $('#Status').val(response.Status);
                    $('#Keyss').val(response.Keyss);
                   // $('#CreatedBy').val(response.CreatedBy);
                    $('#modal-Update').modal('show');
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });
            return false;
        }
        //GetAllDetatilsByRecordId
        function GetRecordDetail(id) {
            $.ajax({
                url: "/LibraryMasterUpload/GetDetailsMasterRecord/" + id,
                typr: "GET",
                contentType: "application/json;charset=UTF-8",
                dataType: "json",
                success: function (response) {
                    $('#RecordId1').val(response.RecordId);
                    $('#ShortName1').val(response.ShortName);
                    $('#DepCode1').val(response.DepCode);
                    $('#DoccumentName1').val(response.DoccumentName);
                    $('#Location1').val(response.Location);
                    $('#EmpName1').val(response.EmpName);
                    $('#Status1').val(response.Status);
                    $('#IssueTo1').val(response.IssueTo);
                    $('#Keyss1').val(response.Keyss);
                    $('#CreatedBy1').val(response.CreatedBy);
                    $('#UpdatedBy1').val(response.UpdatedBy);
                    $('#CreatedOn1').val(response.CreatedOn);
                    $('#UpdatedOn1').val(response.UpdatedOn);
                    $('#model-showdetails').modal('show');
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });

            return false;
        }

