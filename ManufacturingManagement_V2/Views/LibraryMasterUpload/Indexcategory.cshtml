                                            <br />
                                            <label>File Name/Reference Number</label>
                                            <br />
                                            @Html.TextAreaFor(model => model.DoccumentName, 0, 0, new { @class = "Editor", @style = "width:350px;height:25px;" })
                                            <br />
                                            <label>Location</label>
                                            <br />
                                            @Html.TextAreaFor(model => model.Location, 0, 0, new { @class = "Editor", @style = "width:350px;height:25px;" })
                                            @*  @Html.EditorFor(model => model.Location)*@
                                            <br />
                                            <label>Select  Type </label>
                                            <br />
                                            <select name="Type" id="Type">
                                                <option value="Physical">Physical</option>
                                                <option value="Soft">Soft</option>
                                            </select>
                                            <br />
                                            <label>Status </label>
                                            <br />
                                            <select name="Status" id="Status">
                                                <option value="Retainer">With Retainer</option>
                                                <option value="With Owner">With Owner</option>
                                                <option value="Issue To">Issued</option>
                                            </select>
                                            <br />
                                            <label>Select Owner Name</label>
                                            <br />
                                            @Html.DropDownListFor(model => model.NewEmpId, ViewBag.OwnerList as SelectList)
                                            <br />
                                            <label>Keywords</label>
                                            <br />
                                            @Html.TextAreaFor(model => model.Keyss, 0, 0, new { @class = "Editor", @style = "width:350px;height:25px;" })
                                            <br />
                                            @*<br />
                                                <label>Attached File</label>
                                                <br />
                                                @Html.TextBoxFor(model => model.files, "", new { @type = "file", @multiple = "multiple" })*@
                                        </div>
                                    </div>

                                </div>
                                <div class="clearfix"></div>
                            </div>
                            <div class="modal-footer">

                                <button type="submit" id="Apply" class="btn btn-primary">Submit</button>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
