@model ManufacturingManagement_V2.Models.EmployeeMdl
@{
    ViewBag.Title = "EmpList";
    Layout = "~/views/shared/_LayoutCommon.cshtml";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Emp Late List</title>
</head>
<body>
    <p>
      Late Employee List
    </p>
    <section class="content">
        <div class="row">
            <div class="col-md-12">
                <div class="box box-primary">
                    <div class="box-header">
                    </div>
                    <div class="box-body">
                        <div>
                           @Html.ActionLink("Send SMS To Today Late Employeee", "EmpLateSms", "HR", null, new { @class = "btn btn-warning" })
                            <div id="printbar" class="pull-right">
                            </div>
                        </div>
                        <table id="jdtabledesktop" class="display nowrap" style="width:100%">
                            <thead>
                                <tr>
                                    <th>EmpId</th>
                                    <th>Emp Name</th>
                                    <th>lateDate</th>
                                    <th>LateTime</th>
                                    <th>Status</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.Item_List)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(model => item.EmpId)</td>
                                        <td>@Html.DisplayFor(model => item.EmpName)</td>
                                        <td>@Html.DisplayFor(model => item.EDate)</td>
                                        <td>@Html.DisplayFor(model => item.ETime)</td>
                                        <td>L</td>

                                        <td>
                                            <a onclick="GetDetails(@item.NewEmpId)">
                                                <i class="fa fa-edit">
                                                </i>
                                            </a>
                                            @*<a>
                                                    @Html.ActionLink(" ", "LibraryMasterDeleteRecord", "LibraryMasterUpload", new { id = item.Id }, new { onclick = "return confirm('Are you sure wants to delete?');", @class = "fa fa-trash-o" })
                                                </a>*@
                                        </td>
                                    </tr>
                                }
                            </tbody>
                            <tfoot>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <script src="~/Script/Site.js"></script>
</body>
</html>



