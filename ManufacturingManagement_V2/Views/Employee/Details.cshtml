@model ManufacturingManagement_V2.Models.EmployeeMdl

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutCommon.cshtml";
}

<h4><b>Employee Detail</b></h4>
@if (ViewBag.ToEdit == "1")
{
<p>
    @Html.ActionLink("Edit", "CreateUpdate", 
     new { id = Model.NewEmpId }, new { @class = "btn btn-default" })
    
    @Html.ActionLink("Back to List", "Index", null, 
     new { @class = "btn btn-default" })

    @Html.ActionLink("Create New", "CreateUpdate", null, 
     new { @class = "btn btn-default" })

    @Html.ActionLink("Family Detail", "../FamilyDetail/Index",
     new { newempid = Model.NewEmpId, empname = Model.EmpName },
     new { @target = "_blank", @class = "btn btn-default" })

    @Html.ActionLink("Nominee Detail", "../PFNominee/Index",
     new { newempid = Model.NewEmpId, empname = Model.EmpName },
     new { @target = "_blank", @class = "btn btn-default" })

    @Html.ActionLink("Qualification Detail", "../QualificationDetail/Index",
     new { newempid = Model.NewEmpId, empname = Model.EmpName },
     new { @target = "_blank", @class = "btn btn-default" })

    @Html.ActionLink("Experience Detail", "../ExperienceDetail/Index",
     new { newempid = Model.NewEmpId, empname = Model.EmpName },
     new { @target = "_blank", @class = "btn btn-default" })

    @Html.ActionLink("Salary Updation", "../SalaryDetail/Index",
     new { newempid = Model.NewEmpId, empname = Model.EmpName },
     new { @target = "_blank", @class = "btn btn-default" })

    @Html.ActionLink("Promotion Entry", "../PromotionDetail/Index",
     new { newempid = Model.NewEmpId, empname = Model.EmpName },
     new { @target = "_blank", @class = "btn btn-default" })

    @Html.ActionLink("Opening Entry", "../EmployeeOpening/Index",
     new { newempid = Model.NewEmpId, empname = Model.EmpName },
     new { @target = "_blank", @class = "btn btn-default" })

    @Html.ActionLink("Attendance", "../Form1415Report/Index",
     new { newempid = Model.NewEmpId, empname = Model.EmpName },
     new { @target = "_blank", @class = "btn btn-default" })

    @Html.ActionLink("View File(s)", "Downloads",
     new { newempid = Model.NewEmpId, empname = Model.EmpName },
     new { @target = "_blank", @class = "btn btn-default" })

    @Html.ActionLink("Updation Log", "../EventLogReport/getLogDetail",
     new { tblid = 25, pkval = Model.NewEmpId }, new { @target = "_blank", @class = "btn btn-default" })
</p>
}
<fieldset>
    @*<legend>Employee</legend>*@
    @using (Ajax.BeginForm("SearchEmployee", "Employee", new AjaxOptions { UpdateTargetId = "divFilter" }))
    {
        <div class="form-group">
            <table class="table table-bordered">
                <tr>
                    @Html.Hidden("hfNewEmpId", null, new { @id = "hfNewEmpId" })
                    <td>
                        <b>Select Employee</b>
                        &nbsp;&nbsp;@Html.TextBox("txtEmpCode", null,
                        new { placeholder = "Start typing, then select", style = "width:320px;"})
                        
                        <button class="btn btn-default">Search</button>

                        <label id="dh" class="btn btn-default" onclick="displayAll()">Display All</label>
                        
                        @Html.ActionLink("Preview", "", "", null,
                            new { id = "mylink", @target = "_new;", @class = "btn btn-default" })
                    </td>
                </tr>
            </table>
        </div>
    }
</fieldset>

