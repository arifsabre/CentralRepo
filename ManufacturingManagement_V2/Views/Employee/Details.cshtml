@section Scripts {
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
    <script type="text/javascript">
        $(function () {
            $("input[type=text]").first().focus();
            $("#PFApplicable").attr("style", "width:55px;height:25px;");
            $("#ESIApplicable").attr("style", "width:55px;height:25px;");
            $("#IsActive").attr("style", "width:55px;height:25px;");
            //document.getElementById("IsActive").attr("style", "width:55px;height:25px;");
            $("#EmpId").attr("placeholder", "start typing, then select");
            $('#img1').attr('src', '/Employee/ShowEmpImage' + '?newempid='+@Model.NewEmpId);
            document.getElementById("hfNewEmpId").value='@Model.NewEmpId';
            document.getElementById("txtEmpCode").value = '@Model.EmpName';
            $('#mylink').attr('href', '/EmployeeFormsReport/EmployeeFormReportLink?newempid=' + @Model.NewEmpId);
        });
    </script>

    <script type="text/javascript">
        function displayAll() {
            var lbltext = document.getElementById('dh').innerHTML;
            if (lbltext == 'Display All') {
                $('#dh').text('Hide All');
                divJoiningDet.style.display = "block";
                divPaymentDet.style.display = "block";
                divPersonalDet.style.display = "block";
                divContactDet.style.display = "block";
                divEmergencyDet.style.display = "block";
                divReferenceDet.style.display = "block";
                divBankDet.style.display = "block";
                divPfEsiDet.style.display = "block";
                divLeavingDet.style.display = "block";
                divQualDetail.style.display = "block";
                divExpDetail.style.display = "block";
                divFamilyDetail.style.display = "block";
                divNomineeDetail.style.display = "block";
            }
            else if (lbltext == 'Hide All') {
                $('#dh').text('Display All');
                divJoiningDet.style.display = "none";
                divPaymentDet.style.display = "none";
                divPersonalDet.style.display = "none";
                divContactDet.style.display = "none";
                divEmergencyDet.style.display = "none";
                divReferenceDet.style.display = "none";
                divBankDet.style.display = "none";
                divPfEsiDet.style.display = "none";
                divLeavingDet.style.display = "none";
                divQualDetail.style.display = "none";
                divExpDetail.style.display = "none";
                divFamilyDetail.style.display = "none";
                divNomineeDetail.style.display = "none";
            }
        }
    </script>

    <script type="text/javascript">
        function ShowHideDiv(id) {
            if (id.style.display == "block") {
                id.style.display = "none";
            }
            else {
                id.style.display = "block";
            }
        }
    </script>

    <script type="text/javascript">
        $("#txtEmpCode").autocomplete({
            source: function (request, response) {
                var customer = new Array();
                $.ajax({
                    async: false,
                    cache: false,
                    type: "POST",
                    url: "@(Url.Action("AutoCompleteEmployeeNew", "HelpUtility"))",
                    data: { "term": request.term },
                    success: function (data) {
                        for (var i = 0; i < data.length ; i++) {
                            customer[i] = { label: data[i].Value, Id: data[i].Key };
                        }
                    }
                });
                response(customer);
            }, minLength: 2,
            select: function (event, ui) {
                document.getElementById('hfNewEmpId').value = ui.item.Id;
                $('#mylink').attr('href', '/EmployeeFormsReport/EmployeeFormReportLink?newempid=' + ui.item.Id);
            }
        });
    </script>

}

