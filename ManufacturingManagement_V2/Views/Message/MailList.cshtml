@model ManufacturingManagement_V2.Models.MessageModel
@{
// Layout = null;
// Layout = "~/Views/Shared/_LayoutCommon1.cshtml";
// Layout = "~/views/shared/_LayoutIFramePopup.cshtml";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>MailList</title>
    <link href="~/Contents/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Script/Site.js"></script>
</head>
<body>
    <div>

        @using (Html.BeginForm())
            {
            <div class="modal-header">
                @*<button type="submit" id="TopicList" class="btn btn-success">Submit</button>*@
                <button type="button" class="btn btn-success" data-dismiss="modal">Close</button>
            </div>
            <table id="jdtabledesktop" class="table table-bordered table-hover " style="width:50%;height:600px;color :black">
                <thead>
                    <tr>
                        <th>UserName</th>
                        <th>Email</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.Item_List1)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(model => item.FullName)
                            </td>
                            <td>
                                @Html.DisplayFor(model => item.Email)
                            </td>
                           

                        </tr>
                    }

                </tbody>
                <tfoot>
                </tfoot>
            </table>

        }

    </div>
</body>
</html>
