                        </td>
                        @*<td>
                            @Html.DisplayFor(modelItem => item.condition)
                        </td>*@
                        <td>
                            @Html.DisplayFor(modelItem => item.NextDueDate)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.NotUpdatedSinceDays)
                        </td>

                        @*<td>
