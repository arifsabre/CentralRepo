                    $('#FinalApprovalDate2').val(response.FinalApprovalDate);
                    $('#FinalRemark2').val(response.FinalRemark);
                    $('#WorkRating2').val(response.WorkRating);
                    $('#AssignTo2').val(response.AssignTo);
                    $('#FullName2').val(response.FullName2);
                    $('#modal-viewdetailhodsection').modal('show');

