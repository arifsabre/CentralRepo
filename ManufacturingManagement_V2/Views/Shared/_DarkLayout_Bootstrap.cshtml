    <style>
        .collapse {
            width: 15px;
            background-image: url('../Images/ui-icons_cd0a0a_256x240.png');
            background-repeat: no-repeat;
            background-position: -36px -17px;
            display: inline-block;
            /*cursor: zoom-in;*/
            cursor: pointer;
        }

        .expand {
            width: 15px;
            background-image: url('../Images/ui-icons_cd0a0a_256x240.png');
            background-repeat: no-repeat;
            background-position: -50px -17px;
            display: inline-block;
            cursor: pointer;
            /*cursor: zoom-in;*/
        }

        .treeview ul {
            font: 16px Arial, Sans-Serif;
            margin: 5px;
            padding-left: 20px;
            list-style: none;
        }

        .treeview > li > a {
            font-weight: normal;
        }

        .treeview li {
        }

            .treeview li a {
                padding: 5px;
                font-size: 14px;
                display: inline-block;
                text-decoration: none;
                width: auto;
            }

        header {
