            width: 220px;
            height: auto;
            background-color: whitesmoke;
            font-size: small;
            border: solid;
            border-color: whitesmoke;
            font-family: Calibri;
            margin-left: 0px;
            margin-right: 20px;
        }

        .content {
            background-color: white;
            padding: 0px;
            color: black;
        }

        footer {
            width: 20%;
            height: 35px;
            float: none;
            text-align: center;
            vertical-align: central;
            position: fixed;
            bottom: 0px;
        }

        /*fortopheaderhorizontal*/
        body {
            margin: 0;
        }

        .icon-bar {
            width: 100%;
            background-color: teal;
            overflow: auto;
        }

            .icon-bar a {
                float: left;
                width: 3%;
                text-align: center;
                padding: 12px 0;
                transition: all 0.3s ease;
                color: white;
                font-size: 20px;
                /*margin-left:55px;*/
            }

                .icon-bar a:hover {
                    background-color: mediumvioletred;
                }

        .active {
            background-color: #4CAF50;
        }
        /*ForDropdown Hover*/
        .dropbtn {
            background-color: lightgray; /*background-color: #4CAF50;*/
            color: black;
            padding: 5px;
            font-size: small;
            border: none;
            border-radius: 10px;
            font-family: Calibri;
        }

        .dropdown {
            position: relative;
            display: inline-block;
        }

        .dropdown-content {
            display: none;
            position: absolute;
            background-color: navajowhite;
            min-width: 120px;
            box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
            z-index: 1;
            border-radius: 10px;
        }

            .dropdown-content a {
                color: black;
                padding: 5px 8px;
                text-decoration: none;
                display: block;
                text-align: left;
                border-radius: 10px;
            }

                .dropdown-content a:hover {
                    background-color: #ddd;
                }

        .dropdown:hover .dropdown-content {
            display: block;
        }

        .dropdown:hover .dropbtn {
            background-color: #3e8e41;
            border-radius: 10px;
            color: white;
        }
        /*End of dropdown hover*/

        #img1 {
            height: 35px;
            width: 100px;
        }

        #img2 {
            width: 70px;
            height: 50px;
        }

        #hr {
            width: 25px;
            height: 25px;
        }

        #img3 {
            width: 60px;
            height: 60px;
        }

        #img4 {
            height: 40px;
            width: 120px;
        }

        #img5 {
            height: 25px;
            width: 25px;
        }

        #logout {
            height: 30px;
            width: 35px;
        }

        #add {
            height: 35px;
            width: 35px;
        }

        #network {
            height: 20px;
            width: 20px;
        }

        /* Style the links inside the sidenav */
        #mySidenav a {
            position: absolute; /* Position them relative to the browser window */
            right: -80px; /* Position them outside of the screen */
            transition: 0.6s; /* Add transition on hover */
            padding: 15px; /* 15px padding */
            width: 100px; /* Set a specific width */
            text-decoration: none; /* Remove underline */
            font-size: small; /* Increase font size */
            color: white; /* White text color */
            border-radius: 0 5px 5px 0; /* Rounded corners on the top right and bottom right side */
            margin-top: 28px;
            text-align: right;
        }

            #mySidenav a:hover {
                right: 0; /* On mouse-over, make the elements appear as they should */
            }

        /* The about link: 20px from the top with a green background */
        #company {
            top: 80px;
            background-color: #4CAF50;
        }

        #policy {
            top: 140px;
            background-color: #2196F3; /* Blue */
        }

        #notification {
            top: 200px;
            background-color: #f44336;
        }

        #mannual {
